  "devDependencies": {
    "abstract-log": "^3.0.3",
    "jsdoc-to-markdown": "~3.0.0",
    "nyc": "^10.1.2",
    "semistandard": "*",
    "sqlite3": "^3.1.8",
    "tape": "^4.6.3",
    "tape-promise": "^2.0.1"
  },
  "keywords": [],
  "nyc": {
    "exclude": [
      "coverage",
